{"ast":null,"code":"import CustomInput from '@/components/CustomInput';\nimport ActionButtons from '@/components/ActionButtons';\nexport default {\n  name: 'SpendingTableItem',\n  components: {\n    CustomInput,\n    ActionButtons\n  },\n  props: {\n    expense: {\n      type: Object,\n      default: () => ({})\n    },\n    isEditing: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      editingExpense: {}\n    };\n  },\n  created() {\n    this.init();\n  },\n  watch: {\n    isEditing() {\n      this.init();\n    }\n  },\n  methods: {\n    init() {\n      console.log('init: ', this.expense);\n      this.editingExpense = JSON.parse(JSON.stringify(this.expense));\n    },\n    changeField(fieldName, value) {\n      console.log('changeField fieldName, value: ', fieldName, value);\n      this.editingExpense[fieldName] = value;\n    },\n    setEditingExpense() {\n      console.log('setEditingExpense this.editingExpense: ', this.editingExpense);\n      this.$emit('set-editing-expense-id', this.expense.id);\n    },\n    cancelEditingExpense() {\n      this.$emit('set-editing-expense-id', '');\n    },\n    updateExpense() {\n      this.$emit('update-expense', this.editingExpense);\n    },\n    removeExpense() {\n      this.$emit('remove-expense', this.expense.id);\n    }\n  }\n};","map":{"version":3,"names":["CustomInput","ActionButtons","name","components","props","expense","type","Object","default","isEditing","Boolean","data","editingExpense","created","init","watch","methods","console","log","JSON","parse","stringify","changeField","fieldName","value","setEditingExpense","$emit","id","cancelEditingExpense","updateExpense","removeExpense"],"sources":["/Users/uriiponomarenko/Downloads/Accounting-change-backend/js/src/pages/Spending/SpendingTableItem.vue"],"sourcesContent":["<template>\n  <tr\n    class=\"spending-table-item\"\n    :class=\"{ 'not-editable': !isEditing }\"\n  >\n    <td>\n      <span class=\"text\">{{ editingExpense.foodExpense }}</span>\n      <custom-input\n        :id=\"`${editingExpense.id}-${editingExpense.foodExpense}`\"\n        :value=\"editingExpense.foodExpense\"\n        @change-field=\"({ value }) => changeField('foodExpense', value)\"\n      />\n    </td>\n    <td>\n      <span class=\"text\">{{ editingExpense.utilityExpense }}</span>\n      <custom-input\n        :id=\"`${editingExpense.id}-${editingExpense.utilityExpense}`\"\n        :value=\"editingExpense.utilityExpense\"\n        @change-field=\"({ value }) => changeField('utilityExpense', value)\"\n      />\n    </td>\n    <td>\n      <span class=\"text\">{{ editingExpense.otherExpense }}</span>\n      <custom-input\n        :id=\"`${editingExpense.id}-${editingExpense.otherExpense}`\"\n        :value=\"editingExpense.otherExpense\"\n        @change-field=\"({ value }) => changeField('otherExpense', value)\"\n      />\n    </td>\n    <td>\n      <span class=\"text\">{{ editingExpense.dateExpensePersonal }}</span>\n      <custom-input\n        :id=\"`${editingExpense.id}-${editingExpense.dateExpensePersonal}`\"\n        :value=\"editingExpense.dateExpensePersonal\"\n        type=\"date\"\n        @change-field=\"({ value }) => changeField('dateExpensePersonal', value)\"\n      />\n    </td>\n    <td>\n      <action-buttons\n        :is-editing=\"isEditing\"\n        @set-editing=\"setEditingExpense\"\n        @cancel-editing=\"cancelEditingExpense\"\n        @remove=\"removeExpense\"\n        @save=\"updateExpense\"\n      />\n    </td>\n  </tr>\n</template>\n\n<script>\nimport CustomInput from '@/components/CustomInput'\nimport ActionButtons from '@/components/ActionButtons'\n\nexport default {\n  name: 'SpendingTableItem',\n\n  components: {\n    CustomInput,\n    ActionButtons\n  },\n\n  props: {\n    expense: {\n      type: Object,\n      default: () => ({})\n    },\n    isEditing: {\n      type: Boolean,\n      default: false\n    }\n  },\n\n  data() {\n    return {\n      editingExpense: {}\n    }\n  },\n\n  created() {\n    this.init()\n  },\n\n  watch: {\n    isEditing() {\n      this.init()\n    }\n  },\n\n  methods: {\n    init() {\n      console.log('init: ', this.expense);\n      this.editingExpense = JSON.parse(JSON.stringify(this.expense))\n    },\n\n    changeField(fieldName, value) {\n      console.log('changeField fieldName, value: ', fieldName, value);\n      this.editingExpense[fieldName] = value\n    },\n\n    setEditingExpense() {\n      console.log('setEditingExpense this.editingExpense: ', this.editingExpense);\n      this.$emit('set-editing-expense-id', this.expense.id)\n    },\n\n    cancelEditingExpense() {\n      this.$emit('set-editing-expense-id', '')\n    },\n\n    updateExpense() {\n      this.$emit('update-expense', this.editingExpense)\n    },\n\n    removeExpense() {\n      this.$emit('remove-expense', this.expense.id)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.spending-table-item {\n  .text {\n    display: none;\n    padding: 12px 10px;\n  }\n\n  .custom-input {\n    padding: 0;\n  }\n\n  &.not-editable {\n    .text {\n      display: block;\n    }\n\n    .custom-input {\n      display: none;\n    }\n  }\n}\n</style>"],"mappings":"AAmDA,OAAOA,WAAU,MAAO,0BAAyB;AACjD,OAAOC,aAAY,MAAO,4BAA2B;AAErD,eAAe;EACbC,IAAI,EAAE,mBAAmB;EAEzBC,UAAU,EAAE;IACVH,WAAW;IACXC;EACF,CAAC;EAEDG,KAAK,EAAE;IACLC,OAAO,EAAE;MACPC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAEA,CAAA,MAAO,CAAC,CAAC;IACpB,CAAC;IACDC,SAAS,EAAE;MACTH,IAAI,EAAEI,OAAO;MACbF,OAAO,EAAE;IACX;EACF,CAAC;EAEDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,CAAC;IACnB;EACF,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,IAAI,CAAC;EACZ,CAAC;EAEDC,KAAK,EAAE;IACLN,SAASA,CAAA,EAAG;MACV,IAAI,CAACK,IAAI,CAAC;IACZ;EACF,CAAC;EAEDE,OAAO,EAAE;IACPF,IAAIA,CAAA,EAAG;MACLG,OAAO,CAACC,GAAG,CAAC,QAAQ,EAAE,IAAI,CAACb,OAAO,CAAC;MACnC,IAAI,CAACO,cAAa,GAAIO,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,SAAS,CAAC,IAAI,CAAChB,OAAO,CAAC;IAC/D,CAAC;IAEDiB,WAAWA,CAACC,SAAS,EAAEC,KAAK,EAAE;MAC5BP,OAAO,CAACC,GAAG,CAAC,gCAAgC,EAAEK,SAAS,EAAEC,KAAK,CAAC;